local FluentRedz = loadstring(game:HttpGet("https://raw.githubusercontent.com/thieengw9990/Libary-con-cak-dumem/refs/heads/main/libary%20test"))()

-- Create the main window
local window = FluentRedz:CreateWindow("My UI", {
    Theme = "Dark"  -- or lights
})

-- Tạo các tab
local mainTab = window:CreateTab("Main", nil)
local settingsTab = window:CreateTab("Settings", nil)
local playerTab = window:CreateTab("Player", nil)

-- Add controls to tabs
local button = FluentRedz.Controls.CreateButton(mainTab, {
    Title = "Click Me",
    Callback = function()
        print("Button clicked!")
    end
})

local toggle = FluentRedz.Controls.CreateToggle(mainTab, {
    Title = "Enable Feature",
    Default = false,
    Callback = function(value)
        print("Toggle:", value)
    end
})

-- Add more controls
local slider = FluentRedz.Controls.CreateSlider(mainTab, {
    Title = "Walk Speed",
    Min = 16,
    Max = 200,
    Default = 16,
    Callback = function(value)
        game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = 999
    end
})
